{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "format": "prettier --ignore-path .gitignore --write '**/*.{js,ts,tsx,json}'",
        "lint": "eslint .",
        "precommit": "yarn lint && yarn format",
        "test": "echo 'Error: no test specified'",
        "prepare": "cd .. && husky frontend/.husky"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.22.0",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@vitejs/plugin-react": "^2.0.1",
        "eslint": "^9.22.0",
        "eslint-plugin-react": "^7.37.4",
        "globals": "^16.0.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.4.3",
        "prettier": "^3.5.3",
        "sass": "^1.85.1",
        "typescript": "^4.6.4",
        "typescript-eslint": "^8.26.0",
        "vite": "^3.0.7",
        "vite-plugin-eslint": "^1.8.1"
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx,json,md}": "yarn precommit"
    }
}
